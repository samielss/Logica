algoritmo "somar_numeros"

var
    p1, p2, p3, p4, resp : real
inicio
// Seção de Comandos 
escreval ("Calculadora de média")
escreva ("Escreva primeira média")
leia (p1)
escreva ("Escreva segunda média")
leia (p2)
escreva ("Escreva terceira média")
leia (p3)
escreva ("Escreva quarta média")
leia (p4)
resp <- ((p1+p2+p3+p4)/4)
escreva ("A média final é", resp)
fimalgoritmo
